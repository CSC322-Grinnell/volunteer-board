<div class=margin-sides>
  
  <!-- lists all of the users in order of last name with a link to their profile if the viewer is an organization -->
    
<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
<% if organization_signed_in? %>
  <tbody>
    <% (@users.order(:last_name)).each do |user| %>
      <tr>
        <td><a href=<%="users/#{user.id}"%>><%= user.first_name %></a></td>
        <td><%= user.last_name %></td>
      </tr>
    <% end %>
  </tbody>
  
<% else %>
  <tbody>
    You do not have access to this page, please log in as an organization to view the users
  </tbody>
<% end %>

</table>
</div>