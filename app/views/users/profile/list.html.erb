<p id="notice"><%= notice %></p>
  
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
</head>
<body>
 
<div class=margin-sides>
<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
<% if organization_signed_in? %>
  <tbody>
    <% (@users.order(:last_name)).each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <!-- we need to insert a view profile button in here -->
        <td><a href=<%="users/#{user.id}"%>>View Profile </a></td>
      </tr>
    <% end %>
  </tbody>
  
<% else %>
  <tbody>
    You do not have access to this page, please log in as an organization to view the users
  </tbody>
<% end %>

</table>
</div>